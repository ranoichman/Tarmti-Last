<body>

    <div id="signupPH" class="container-fluid"></div>

    <script type="text/babel">

        var Signup = React.createClass({
            render: function () {
                var msg;
                if (true) {
                    msg = 'checked';        
                }
                return(
                    <div>
                        <input type="checkbox"/>
                    <h3> Checkbox is {msg} </h3>
                </div>
                   );
            }
        });
        ReactDOM.render(<div><Signup></Signup></div>,document.getElementById('signupPH'));

    </script>
</body>
